.meet-header {
    width: 100%;
    margin-bottom: 50px;
    border-bottom: 1px solid var(--color__light-grey);
    &__meet {
        display: flex;
        align-items: center;
    }
    &__item{
        padding-bottom: 15px;
        width: max-content;
        font-size: 15px;
        margin: 0 30px -1px 0;
        &_active{
            border-bottom: 2px solid var(--color__yellow)!important;
        }
    }
}
